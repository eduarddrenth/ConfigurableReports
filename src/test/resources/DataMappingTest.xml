<dm:datamappings
   xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
   xmlns:dm='http://www.vectorprint.nl/report/datamapping/1.3'>
   <dm:datamapping classname=".*\.My[^.]*" regex="true">
      <dm:element>
         <dm:datatype dataclass="com.vectorprint.report.data.types.DurationValue" format="#,### 'hours'"/>
         <dm:styleclass>niks</dm:styleclass>
         <dm:elementtype>Phrase</dm:elementtype>
      </dm:element>
      <dm:elementsfromdata>
         <dm:element>
            <dm:elementtype>Chunk</dm:elementtype>
         </dm:element>
         <dm:datalistmethod>getMeFirst</dm:datalistmethod>
      </dm:elementsfromdata>
   </dm:datamapping>
   <dm:datamapping classname=".*\.My[^.]*" regex="true" id="idee">
      <dm:element>
         <dm:datatype dataclass="com.vectorprint.report.data.types.DurationValue"/>
         <dm:elementtype>Phrase</dm:elementtype>
      </dm:element>
      <dm:elementsfromdata>
         <dm:element>
            <dm:elementtype>Chunk</dm:elementtype>
         </dm:element>
         <dm:datalistmethod>getMe</dm:datalistmethod>
      </dm:elementsfromdata>
   </dm:datamapping>

</dm:datamappings>
